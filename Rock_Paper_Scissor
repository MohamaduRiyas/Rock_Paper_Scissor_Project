#include<iostream>
#include<string>
using namespace std;

int main()
{
    string n;
    cout<<"Enter a string:";
    cin>>n;
    
    if(n == "Rock" || n == "rock"){
        cout<<"Paper";
    }
    else if(n == "Paper" || n == "paper"){
        cout<<"Scissor";
    }
    else{
        cout<<"Rock";
    }
    return 0;
}
